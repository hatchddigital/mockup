<td class="selection"><input type="checkbox" <% if(selected){ %> checked="checked" <% } %>/></td>
<td class="title">
  <% if(attributes.is_folderish){ %>
    <a href="<%- getURL %>">
        <span class="icon icon-folder-open"></span> <%- Title %>
    </a>
  <% } else { %>
    <% if(attributes.Type === 'Image'){ %>
        <% if (typeof(thumbnailURL) !== 'undefined') { %>
            <span class="icon js--thumbnail-thumb" id="thumbnail-hover-thumb"><img src="<%- thumbnailURL %>" /></span>
        <% } else { %>
           <span class="icon icon-picture"></span>
        <% } %>
    <% } else { %>
        <% if(attributes.Type === 'File'){ %>
            <span class="icon icon-file"></span>
        <% } %>
    <% } %>
    <%- Title %>
  <% } %>
</td>
<% if(showUseMenu){ %>
    <td><a href="#" class="use">Use</a></td>
<% } %>
<% _.each(activeColumns, function(column){ %>
  <% if(_.has(availableColumns, column)) { %>
    <td class="<%- column %>"><%- attributes[column] %></td>
  <% } %>
<% }); %>

